{
    "parser": "babel-eslint",
    "rules": {
        "no-unused-vars": "error",
        "no-console": 1, // Means warning
        "no-debugger": 2,
        "func-names": 0,
        "import/extensions": 0,
        "no-unused-expressions": "error",
        "no-shadow": "off"
    },
    "env": {
        "es6": true,
        "browser": true,
        "jest": true
    },
    "extends": ["eslint:recommended", "prettier"]
}
